<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="perfil_circular">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="getStartedTitulo">
        <item name="android:fontFamily">@font/poppins700</item>
        <item name="android:textSize">24sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_marginEnd">20dp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="getStartedTexto">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginStart">30dp</item>
        <item name="android:layout_marginEnd">30dp</item>
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/getStartedTextColor</item>
    </style>
    <style name="botonCustom">
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/boton_redondo_animado</item>
        <item name="android:gravity">center</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>